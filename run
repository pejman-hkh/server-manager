if [ "$1" = "php" ]; then
	#echo "php";
	php/php-8.1.12/bin/php ${*:2}
elif [ "$1" = "nginx" ]; then
	nginx/nginx-1.22.1/objs/nginx ${*:2}
elif [ "$1" = "apache" ]; then	
	apache/httpd-2.4.54/bin/httpd ${*:2}
elif [ "$1" = "mysql" ]; then	
	echo "mysql";
elif [ "$1" = "nodejs" ]; then	
	nodejs/node-v18.12.1/bin/node ${*:2}
elif [ "$1" = "python" ]; then	
	python/Python-3.11.0/bin/python3
elif [ "$1" = "bind" ]; then	
	echo "bind";
elif [ "$1" = "composer" ]; then	
	echo "composer";
fi